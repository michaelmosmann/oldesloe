{% extends "parts/_page" %}
{% import "parts/_shops" %}

{% block content %}
    {% for doc in it.documents %}
    <h2>{{ doc.name | single }}</h2>
    {{ onlineShopLink(doc) }}
    {{ doc }}
    {% set pathToShopPage = doc.pathToPage('shop') %}
    {% if pathToShopPage is not null %}
        <a href="{{ it.baseUrl }}{{ pathToShopPage }}">Details</a>
    {%endif %}
    {% endfor %}
{% endblock %}
